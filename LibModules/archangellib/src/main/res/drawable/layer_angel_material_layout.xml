<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android = "http://schemas.android.com/apk/res/android">
    <item android:id = "@+id/layer_angel_material_layout_background">
        <shape android:shape = "rectangle">
            <solid android:color = "@color/trans" />
        </shape>
    </item>
    <item
        android:id = "@+id/layer_angel_material_layout_foreground"
        android:bottom = "1dp"
        android:left = "1dp"
        android:right = "1dp"
        android:top = "1dp">
        <shape android:shape = "rectangle">
            <corners android:radius = "4dp" />
            <solid android:color = "@color/trans" />
        </shape>
    </item>
</layer-list>