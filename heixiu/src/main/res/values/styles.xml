<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="popupWindow"
        parent="@android:style/Animation.Activity">
        <item name="android:windowEnterAnimation">@anim/popup_in</item>
        <item name="android:windowExitAnimation">@anim/popup_out</item>
    </style>

    <style name="popupWindow2"
        parent="@android:style/Animation.Activity">
        <item name="android:windowEnterAnimation">@anim/popup_out</item>
        <item name="android:windowExitAnimation">@anim/popup_in</item>
    </style>

    <style name="popupWindow1"
        parent="@android:style/Animation.Activity">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
    </style>

    <style name="HXTheme"
        parent="@style/_HXTheme">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="HXTheme.ActionbarOverlay"
        parent="@style/_HXTheme">
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <style name="HXTheme.Fullscreen"
        parent="AngelTheme.Fullscreen">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/trans</item>
        <item name="android:textColorHighlight">@color/hx_main</item>
        <item name="android:textCursorDrawable">@drawable/shape_cursor_main
        </item>

    </style>

    <style name="HXTheme.AppCompat.Dialog"
        parent="Theme.AppCompat.Light.Dialog"/>

    <style name="Theme.AppCompat.FullScreen"
        parent="@style/Theme.AppCompat.Light">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/trans</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="_HXTheme"
        parent="AngelTheme">
        <!--<item name = "android:typeface">monospace</item>-->
        <item name="android:textColorHighlight">@color/white</item>
        <item name="android:textCursorDrawable">@drawable/shape_cursor_main
        </item>

        <item name="android:windowAnimationStyle">@style/AngelActivityAnimation
        </item>
        <!-- 修改的actionbar样式-->
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Icon">
        <item name="android:textColor">#FFF</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="android:shadowColor">#B0000000</item>
        <item name="android:fontFamily"
            tools:targetApi="jelly_bean">sans-serif-condensed
        </item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="chat_content_date_style">
        <item name="android:minHeight">30dp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:text">textcontent</item>
    </style>

    <style name="chat_text_name_style">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">bottom</item>
    </style>

    <style name="dialog"
        parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/trans</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@color/trans</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>

    <style name="dialog1"
        parent="@android:style/Theme.Dialog">
        <!-- Dialog进入及退出动画 -->
        <item name="android:windowAnimationStyle">@style/popupWindow2</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00ffffff</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="dialog2"
        parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00ffffff</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="dialog3"
        parent="@android:style/Theme.Black">
        <item name="android:windowAnimationStyle">@style/popupWindow2</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00ffffff</item>

        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!-- TabHost标签按钮样式 --><style name="tab_button_bottom">

    <item name="android:textColor">#ffffffff</item>
    <item name="android:ellipsize">marquee</item>
    <item name="android:gravity">center_horizontal</item>

    <item name="android:layout_marginTop">2.0dp</item>
    <item name="android:button">@null</item>
    <item name="android:paddingTop">6dp</item>
    <item name="android:drawablePadding">4dp</item>
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1.0</item>
    <item name="android:singleLine">true</item>

    <item name="android:background">@drawable/tab_indicator</item>

    <item name="android:textSize">14sp</item>
    <item name="android:dividerPadding">8dp</item>
    <item name="android:showDividers">middle</item>

    <item name="android:fadingEdge">horizontal</item>
    <item name="android:fadingEdgeLength">8dp</item>
</style>

    <style name="main_tab_bottom">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/select_text_back</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/tab_indicator</item>
        <!--<item name="android:paddingTop">6dp</item>-->
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <!--<item name="android:drawablePadding">2dp</item>-->
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="chat_text_name_style_left">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">left</item>
        <item name="android:maxEms">18</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="chat_text_date_style">
        <item name="android:padding">2dp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="myActionBarStyle"
        parent="android:Widget.ActionBar">
        <item name="android:background">#FFD350</item>
    </style>

    <style name="gift_contributor_total_coin"
        parent="@style/cell_useritem_username">
        <item name="android:textSize">20.0sp</item>
    </style>

